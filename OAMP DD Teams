Division Director 2 = 
VAR CurrentDirector = '4CAST raw'[Division Director]

VAR CONameFromPTS = LOOKUPVALUE(
    'PTS raw'[CO Name], 
    'PTS raw'[4CAST ID Number (PTS)], '4CAST raw'[4CAST ID Number (4CAST)]
)

VAR OAMPTeam = 
    SWITCH(TRUE(),
        CONTAINSROW(
            {"Checchia, Mario (HRSA)", "Hupalar, Jimmy (HRSA)", "Stephan, Anne (HRSA)", "Whitney, Charisse (HRSA)"},
            CONameFromPTS
        ), "Hupalar, Jimmy (HRSA)",

        CONTAINSROW(
            {"Antwi, Kwasi (HRSA)", "Henriquez, Mario (HRSA)", "Grabill, Russell (HRSA)", "Collins, Ronette (HRSA)"},
            CONameFromPTS
        ), "Antwi, Kwasi (HRSA)",

        CONTAINSROW(
            {"Archibald, David (HRSA)", "Petrone, Stephanie (HRSA)", "Dillon, Gina (HRSA)", "Wesley, Helen (HRSA) [X]", "Nilles, Kevin (HRSA)"},
            CONameFromPTS
        ), "Grabill, Russell (HRSA)",

        CONTAINSROW(
            {"Simmons, Saul (HRSA)"},
            CONameFromPTS
        ), "Simmons, Saul (HRSA)",

        BLANK()
    )

RETURN 
IF(
    NOT ISBLANK(CurrentDirector), 
    CurrentDirector,
    OAMPTeam
)