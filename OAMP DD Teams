Division Director 2 = 
VAR CurrentDirector = '4CAST raw'[Division Director]
VAR CONameFromPTS = LOOKUPVALUE(
    'PTS raw'[CO Name], 
    'PTS raw'[4Cast ID Number], '4CAST raw'[4Cast ID Number]
)
VAR DirectorFromPTS = LOOKUPVALUE(
    'PTS raw'[Division Director], 
    'PTS raw'[CO Name], CONameFromPTS
)

RETURN 
IF(
    NOT ISBLANK(CurrentDirector), 
    CurrentDirector,
    DirectorFromPTS
)