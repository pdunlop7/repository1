Division Director 2 = 
VAR CurrentDirector = '4CAST raw'[Division Director]
VAR COName = LOOKUPVALUE(
    'PTS raw'[CO Name], 
    'PTS raw'[4Cast ID Number (PTS)], 
    '4CAST raw'[4Cast ID Number (4CAST)]
)
RETURN 
IF(
    NOT ISBLANK(CurrentDirector), 
    CurrentDirector,
    SWITCH(
        COName,
        "Antwi, Kwasi (HRSA)", "Antwi, Kwasi (HRSA)",
        "Henriquez, Mario (HRSA)", "Antwi, Kwasi (HRSA)",
        "Collins, Ronette (HRSA)", "Antwi, Kwasi (HRSA)",
        "Grabill, Russell (HRSA)", "Grabill, Russell (HRSA)",
        "Archibald, David (HRSA)", "Grabill, Russell (HRSA)",
        "Petrone, Stephanie (HRSA)", "Grabill, Russell (HRSA)",
        "Dillon, Gina (HRSA)", "Grabill, Russell (HRSA)",
        "Wesley, Helen (HRSA) [X]", "Grabill, Russell (HRSA)",
        "Nilles, Kevin (HRSA)", "Grabill, Russell (HRSA)",
        "Hupalar, Jimmy (HRSA)", "Hupalar, Jimmy (HRSA)",
        "Checchia, Mario (HRSA)", "Hupalar, Jimmy (HRSA)",
        "Stephan, Anne (HRSA)", "Hupalar, Jimmy (HRSA)",
        "Whitney, Charisse (HRSA)", "Hupalar, Jimmy (HRSA)",
        "Simmons, Saul (HRSA)", "Simmons, Saul (HRSA)",
        BLANK()
    )
)