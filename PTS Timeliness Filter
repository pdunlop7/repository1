PTS Timeliness Filter = 
 //This is the same as PTS Timeliness Status except it gives a binary "On Schedule" or "Behind Schedule description for use in a filter.
IF('PTS raw'[PTS Phase (Number)] = 1,
//Phase 1
   IF('PTS raw'[PTS Status] = "Initial request to COR to upload documents" ||
      'PTS raw'[PTS Status] = "Initiate Clearances" ||
       'PTS raw'[PTS Status] = "Request COR to fix document issues",
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Complete Draft Procurement Package Due],
               "On Schedule",
               "Behind Schedule"
   )),
    IF('PTS raw'[PTS Status] = "Initiate Clearances",
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Complete Draft Procurement Package Due],
               "On Schedule",
               "Behind Schedule"
           ),
    IF('PTS raw'[PTS Status] = "ITC Clearance",
       IF(ISBLANK('PTS raw'[ITC Clearance Approved Date]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[ITC Clearance Due Date],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[ITC Clearance Approved Date] <= 'PTS raw'[ITC Clearance Due Date],
               "On Schedule",
               "Behind Schedule"
           )
       ),
        IF('PTS raw'[PTS Status] = "OPAE Clearance",
       IF(ISBLANK('PTS raw'[OPAE Clearance Approved Date]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[OPAE Clearance Due Date],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[OPAE Clearance Approved Date] <= 'PTS raw'[OPAE Clearance Due Date],
               "On Schedule",
               "Behind Schedule"
           )
       ),
               IF('PTS raw'[PTS Status] = "OC Clearance",
       IF(ISBLANK('PTS raw'[OC Clearance Approved Date]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[OC Clearance Due Date],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[OC Clearance Approved Date] <= 'PTS raw'[OC Clearance Due Date],
               "On Schedule",
               "Behind Schedule"
           )
       ),
                IF('PTS raw'[PTS Status] = "OAM Clearance",
       IF(ISBLANK('PTS raw'[OAM Clearance Approved Date]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[OAM Clearance Due Date],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[OAM Clearance Approved Date] <= 'PTS raw'[OAM Clearance Due Date],
               "On Schedule",
               "Behind Schedule"
           )
       ),
                IF('PTS raw'[PTS Status] = "Documents sent to OAMP Review",
       IF(ISBLANK('PTS raw'[Complete Draft Procurement Package Submitted Date]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Complete Draft Procurement Package Due],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[Complete Draft Procurement Package Submitted Date] <= 'PTS raw'[Complete Draft Procurement Package Due],
               "On Schedule",
               "Behind Schedule"
           )
       ),


//Phase 2
   
   IF('PTS raw'[PTS Status] = "PreSolicitation",
       IF(ISBLANK('PTS raw'[Actual PreSolicitation]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Baseline PreSolicitation],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[Actual PreSolicitation] <= 'PTS raw'[Baseline PreSolicitation],
               "On Schedule",
               "Behind Schedule"
           )
       ),
   IF('PTS raw'[PTS Status] = "Solicitation Issued on GPE (FBO or GSA eBuy)",
       IF(ISBLANK('PTS raw'[Actual Solicitation Issued on GPE (FBO or GSA eBuy)]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Baseline Solicitation Issued on GPE (FBO or GSA eBuy)],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[Actual Solicitation Issued on GPE (FBO or GSA eBuy)] <= 'PTS raw'[Baseline Solicitation Issued on GPE (FBO or GSA eBuy)],
               "On Schedule",
               "Behind Schedule"
           )
       ),
   IF('PTS raw'[PTS Status] = "Proposal/Quotations Received",
       IF(ISBLANK('PTS raw'[Actual Proposals/Quotations Received]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Baseline Proposals/Quotations Received],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[Actual Proposals/Quotations Received] <= 'PTS raw'[Baseline Proposals/Quotations Received],
               "On Schedule",
               "Behind Schedule"
           )
       ),
   IF('PTS raw'[PTS Status] = "Pre-Negotiations Memorandum",
       IF(ISBLANK('PTS raw'[Actual Pre-Negotiations Memorandum]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Baseline Pre-Negotiations Memorandum],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[Actual Pre-Negotiations Memorandum] <= 'PTS raw'[Baseline Pre-Negotiations Memorandum],
               "On Schedule",
               "Behind Schedule"
           )
       ),
   IF('PTS raw'[PTS Status] = "Negotiations Start",
       IF(ISBLANK('PTS raw'[Actual Negotiations Start]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Baseline Negotiations Start],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[Actual Negotiations Start] <= 'PTS raw'[Baseline Negotiations Start],
               "On Schedule",
               "Behind Schedule"
           )
       ),
   IF('PTS raw'[PTS Status] = "TEP Consensus Meeting",
       IF(ISBLANK('PTS raw'[Actual TEP Consensus Meeting]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Baseline TEP Consensus Meeting],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[Actual TEP Consensus Meeting] <= 'PTS raw'[Baseline TEP Consensus Meeting],
               "On Schedule",
               "Behind Schedule"
           )
       ),
   IF('PTS raw'[PTS Status] = "Summary/ Source Selection Approved",
       IF(ISBLANK('PTS raw'[Actual Summary/ Source Selection Approved]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Baseline Summary/ Source Selection Approved],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[Actual Summary/ Source Selection Approved] <= 'PTS raw'[Baseline Summary/ Source Selection Approved],
               "On Schedule",
               "Behind Schedule"
           )
       ),
   IF('PTS raw'[PTS Status] = "Award Issued",
       IF(ISBLANK('PTS raw'[Proposed Award Date]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Baseline Award Issued],
               "Awarded On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[Proposed Award Date]<= 'PTS raw'[Baseline Award Issued],
               "Awarded On Schedule",
               "Behind Schedule"
           )
       ),
   IF('PTS raw'[PTS Status] = "Complete Procurement Package",
       IF(ISBLANK('PTS raw'[Actual Complete Procurement Package]),
           IF('Last Refreshed Date'[LastRefreshedDateMeasure] <= 'PTS raw'[Baseline Complete Procurement Package],
               "On Schedule",
               "Behind Schedule"
           ),
           IF('PTS raw'[Actual Complete Procurement Package] <= 'PTS raw'[Baseline Complete Procurement Package],
               "On Schedule",
               "Behind Schedule"
           )
       ),
   ""))))))))))))))))
